/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

void	ft_putchar_fd(char c, int fd)
{
	write(fd, &c, 1);
}

/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_putendl_fd.c                                    :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: shmimi <shmimi@student.1337.ma>            +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2022/10/25 00:15:18 by shmimi            #+#    #+#             */
/*   Updated: 2023/02/12 12:09:42 by shmimi           ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

void	ft_putendl_fd(char *s, int fd)
{
	int	i;

	i = 0;
	if (s)
	{
		while (i < ft_strlen(s))
			write(fd, &s[i++], 1);
		write(fd, "\n", 1);
	}
}

/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_putnbr_fd.c                                     :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: shmimi <shmimi@student.1337.ma>            +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2022/10/25 00:15:21 by shmimi            #+#    #+#             */
/*   Updated: 2022/10/27 00:41:38 by shmimi           ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

void	ft_putnbr_fd(int n, int fd)
{
	if (n >= 0 && n < 10)
		ft_putchar_fd(n + 48, fd);
	else if (n > 9)
	{
		ft_putnbr_fd(n / 10, fd);
		ft_putchar_fd((n % 10) + 48, fd);
	}
	if (n == -2147483648)
	{
		ft_putchar_fd('-', fd);
		ft_putchar_fd('2', fd);
		ft_putnbr_fd(147483648, fd);
	}
	else if (n < 0)
	{
		ft_putchar_fd('-', fd);
		ft_putnbr_fd(-n, fd);
	}
}

/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_putstr_fd.c                                     :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: shmimi <shmimi@student.1337.ma>            +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2022/10/25 00:15:23 by shmimi            #+#    #+#             */
/*   Updated: 2023/02/12 12:09:55 by shmimi           ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

void	ft_putstr_fd(char *s, int fd)
{
	int	i;

	if (s)
	{
		i = 0;
		while (i <= ft_strlen(s) && s[i])
			write(fd, &s[i++], 1);
	}
}

/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_split.c                                         :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: shmimi <shmimi@student.1337.ma>            +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2022/10/25 00:15:26 by shmimi            #+#    #+#             */
/*   Updated: 2023/04/08 20:45:57 by shmimi           ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

static int	count_words(char *s, char delimeter)
{
	int	i;
	int	count;

	i = 0;
	count = 0;
	while (s[i] != '\0')
	{
		while (s[i] == delimeter && s[i])
			i++;
		if (s[i] == '\0')
			break ;
		while (s[i] != delimeter && s[i])
			i++;
		count++;
	}
	return (count);
}

static int	count_chars(char *s, char c, int *st)
{
	int	i;

	i = 0;
	while (s[*st] != c && s[*st])
	{
		i++;
		(*st)++;
	}
	return (i);
}

static void	assign(char *s, char delimeter, char **tab, int word_counts)
{
	int	i;
	int	fixed;
	int	second_dimension;

	i = 0;
	fixed = 0;
	while (i < word_counts)
	{
		second_dimension = 0;
		while (s[fixed] == delimeter && s[fixed])
			fixed++;
		while (s[fixed] != delimeter && s[fixed])
			tab[i][second_dimension++] = s[fixed++];
		tab[i][second_dimension] = 0;
		i++;
	}
	tab[i] = 0;
}

char	**ft_split(char const *s, char c)
{
	int		i;
	char	**tab;
	int		cw;
	int		st;

	i = 0;
	st = 0;
	if (s)
	{
		cw = count_words((char *)s, c);
		tab = (char **)malloc(sizeof(char *) * (cw + 1));
		if (!tab)
			return (0);
		while (i < cw)
		{
			while (s[st] == c && s[st])
				st++;
			tab[i++] = (char *)malloc(count_chars((char *)s, c, &st) + 1);
			if (!s[i - 1])
				return (0);
		}
		assign((char *)s, c, tab, cw);
		return (tab);
	}
	return (0);
}

